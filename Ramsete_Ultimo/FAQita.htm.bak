						wSTIA(6) = 0.198<br>
						wSTIA(7) = 0.337<br>
						wSTIA(8) = 0.241<br>
						wSTIA(9) = 0&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; '8 kHz <br>
						<br>
						wSTIL(3) = 0.13&nbsp;&nbsp;&nbsp; '125 Hz<br>
						wSTIL(4) = 0.14<br>
						wSTIL(5) = 0.11<br>
						wSTIL(6) = 0.12<br>
						wSTIL(7) = 0.19<br>
						wSTIL(8) = 0.17<br>
